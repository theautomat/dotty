# Dotty Environment Variables Template
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit .env to git!

# Firebase Configuration
# Get these values from your Firebase project settings
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# FingerprintJS Pro (optional - for leaderboard user identification)
# Get your API key from https://dashboard.fingerprint.com/
VITE_FINGERPRINT_API_KEY=your_fingerprintjs_api_key

# Solana Configuration
SOLANA_NETWORK=devnet
SOLANA_WALLET_PATH=./wallets/devnet-deployer.json
SOLANA_PROGRAM_ID=Fg6PaFpoGXkYsidMpWTK6W2BeZ7FEfcYkg476zPFsLnS

# Server Configuration (optional)
PORT=3000
NODE_ENV=development
